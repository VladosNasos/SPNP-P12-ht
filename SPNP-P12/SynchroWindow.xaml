<Window x:Class="SPNP_P12.SynchroWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SPNP_P12"
        mc:Ignorable="d"
         Title="SynchroWindow" Height="450" Width="300" WindowStartupLocation="CenterScreen">

    <StackPanel>
        <TextBlock Text="Synchro"/>
        <Button x:Name="StartBtn" Content="Start" Click="StartBtn_Click"/>
        <TextBlock x:Name="logTextBlock"/>
    </StackPanel>
</Window>
